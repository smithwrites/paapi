<div id="<%= dom_id csv_upload %>" data-controller="csv-processing-messages">
  <h2 class="govuk-heading-m">
    
    <%= csv_upload.title %>
  </h2>
  <h3 class="govuk-heading-s">Progress</h3>
    <ul data-csv-processing-messages-target="messages" class="govuk-list govuk-list--bullet">


    <% csv_upload.csv_processing_messages.each do |msg| %>
      <li data-message-id="<%= msg.id %>"><%= msg.body %></li>
    <% end %>
    </ul>
  
  <h3 class="govuk-heading-s">Attached files
  <ul class="govuk-list govuk-list--bullet">
    <% csv_upload.csv_files.each do |csv| %>
       <li><%= link_to csv.filename, csv %></li>
    <% end %>
  </ul>
  </p>
</div>

